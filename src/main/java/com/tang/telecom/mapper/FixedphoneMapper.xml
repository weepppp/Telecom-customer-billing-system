<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tang.telecom.mapper.FixedphoneMapper">

    <select id="getByFixedPhoneNumber" resultType="com.tang.telecom.entity.Fixedphone">
        select * from fixedphone where fixedPhoneNumber = #{fixedPhoneNumber}
    </select>
</mapper>